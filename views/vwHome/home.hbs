<div>
  <table class="table table-striped table-inverse table-responsive">
    <thead class="thead-inverse">
      <tr>
        <th>Category</th>
      </tr>
    </thead>
    <tbody>
      {{#each categoryList}}
        <tr>
          <td>{{name}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>

  {{#each courseList}}
    <div class="card">
      <div class="card-header">
        {{name}}
      </div>
      <div class="card-body">
        <h4 class="card-title">{{briefContent}}</h4>
      </div>
      <div class="card-footer text-muted">
        {{price}}
        <a class="btn btn-sm btn-outline-primary" href="/course/detail/{{id}}" role="button">
          <i class="fa fa-eye" aria-hidden="true"></i>
          View detail
        </a>
      </div>
    </div>
  {{/each}}
</div>